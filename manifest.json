{
  "manifest_version": 2,

  "name": "tabhor",
  "description": "We abhor extra tabs",
  "version": "3.4",

  "browser_action": {
    "default_icon": "tabhor.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    //"activeTab",
    //"https://ajax.googleapis.com/",
    "history",
    "tabs"
  ],

   "background": {
    "script":  ["background.js"]
   }


}
